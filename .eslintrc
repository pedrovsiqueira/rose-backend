{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-console": "off",
        "class-methods-use-this": "off",
        "@typescript-eslint/no-unused-vars": ["error"],
        "no-underscore-dangle": "off",
        "no-restricted-syntax": "off"
    }
}
